  (doubleUs 3 5),
  (tail [1]),
  (zip [1,2,3] [4,5,6]),
  (filter (< 2) [1,2,3,4]),
  (++ [1,2,3] [4,5,6]),
  ((. (+ 1) (* 2)) 5),
  (take 5 (repeat 1))
]);
